<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rglucapstone.activefatherhood">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:icon="@mipmap/logo_app"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".AskActivity"></activity>
        <activity android:name=".QuestionActivity"></activity>
        <activity android:name=".AnswerActivity"></activity>
        <activity android:name=".CategoriesActivity"></activity>
        <activity android:name=".questions.ListQuestionsActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".HomeActivity">
            <meta-data
                android:name="android.app.default_searchable"
                android:value=".SearchResultsActivity" />
        </activity>
        <activity android:name=".SearchResultsActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data android:name="android.app.searchable"
                android:resource="@xml/searchable"/>
        </activity>
        <!--  <activity android:name=".InterestActivity"></activity>
          <activity android:name=".DialogInterestActivity"></activity>-->
    </application>

</manifest>
